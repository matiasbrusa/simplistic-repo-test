}*/
@media (min-width: 769px) {
  #monogramming-pop-up .mg-step-content{display:block!important;height:auto!important;}
}
@media (max-width: 768px) {
  
  .mg-small--one-whole{width:100%;}
  .mg-small--one-half{width:50%;}
  .mg-small--one-third{width:33.333%}
  .mg-small--one-fourth{width:25%}

  .mg-small--two-twelfths{width:16.666%;}
  .mg-small--three-twelfths{width:25%;}
  .mg-small--four-twelfths{width:33.333%;}
  
  .mg-grid{display:block!important;}
  .mg-grid__item{display:block!important;}
  
  
  #monogramming-pop-up{width: 400px;}
  #monogramming-pop-up .mg-step-2{padding-left:20px;}
  #monogramming-pop-up .mg-steps > div{padding-bottom:20px;}
  #monogramming-pop-up .mg-steps > div:last-child{padding-bottom:0px;}
  #monogramming-pop-up .mg-steps > .mg-grid__item{border-left:none;padding-left:0;}
  #monogramming-pop-up .mg-step-content{display:none;}
  #monogramming-pop-up .mg-step-1 .mg-step-content{display:block;}
  
  #monogramming-pop-up h1{font-size:14px;}
  #monogramming-pop-up h1 span:first-child{margin-right:5px;}
}


