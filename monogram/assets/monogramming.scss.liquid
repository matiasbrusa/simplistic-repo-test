/* Structure */
.mg-hide{display:none;}

.mg-grid:after{content:''; display:table; clear:both;}
.mg-grid{box-sizing:border-box; list-style:none; margin:0; padding:0; margin-left: -20px;}
.mg-grid__item{box-sizing:border-box; float:left; min-height:1px; vertical-align:top; width:100%; padding-left: 20px;}

.mg-grid-full{margin-left:0}
.mg-grid-full > .mg-grid__item{padding-left:0}

.mg-one-whole{width:100%}
.mg-one-half{width:50%}
.mg-one-third{width:33.333%}
.mg-one-fourth{width:25%}

.mg-two-twelfths{width:16.666%;}
.mg-three-twelfths{width:25%;}
.mg-four-twelfths{width:33.333%;}

.mg-one-fifth{width:20%}
.mg-two-fifths{width:40%}

.mg-three-tenths{width:30%}

.mg-table{display:table;width:100%;}
.mg-table-cell{display:table-cell;float:none;}

#mg-logo{width: auto;display: inline-block;line-height: normal;height: auto;padding: 5px;}

/* Custom */
#monogramming-pop-up{background:#fff;width: 1000px;max-width: 100%;}
#monogramming-pop-up .mg-title{background-color:#5c7598;padding:10px 20px;color:#fff;text-transform:uppercase;}
#monogramming-pop-up .mg-wrap{padding:10px 20px;}
#monogramming-pop-up h1{font-size:15px;letter-spacing:normal;color:#000;margin-bottom:20px;}
#monogramming-pop-up h1 span:first-child{display:inline-block;background:#5c7598;color:#fff;border-radius:100%;height:30px;width:30px;line-height:30px;text-align:center;margin-right:10px;}
#monogramming-pop-up ul{padding:0; list-style:none;margin:0;}
#monogramming-pop-up ul li{margin-bottom:7px;font-size:14px;}
#monogramming-pop-up ul li:last-child{margin-bottom:0;}
#monogramming-pop-up ul li a{color:#000;}
#monogramming-pop-up ul li a:focus{outline:none;}
#monogramming-pop-up ul li img{margin-right:10px;}
#monogramming-pop-up ul li img,
#monogramming-pop-up ul li span{vertical-align:middle;}

/*.mg-steps > .mg-grid__item{position:relative;display: table-cell;float: none;border-right:1px solid #ddd;}
.mg-steps > .mg-grid__item:last-child{border-right:0;}*/

#monogramming-pop-up .mg-steps > .mg-grid__item{border-left:1px solid #ddd;}
#monogramming-pop-up .mg-steps > .mg-grid__item:first-child{border-left:none;}
#monogramming-pop-up #mg-colors li{margin-bottom:20px;}

#monogramming-pop-up #mg-letters{border-radius:0;width:100%;border: 1px solid #ddd;margin-bottom:10px;}
#monogramming-pop-up .mg-btn{border:2px solid #000;display:inline-block;width:100%;line-height:38px;text-align:center;margin-bottom:10px;color:#000;font-size:15px;}
#monogramming-pop-up .mg-btn-confirm{border-color:#5c7598;color:#5c7598}

#monogramming-pop-up .mg-selected{font-weight:bold;}
#monogramming-pop-up .mg-step-2,
#monogramming-pop-up .mg-step-3,
#monogramming-pop-up .mg-step-4{padding-left:30px;}

#monogramming-pop-up .mg-step-blocked{position:relative;}
#monogramming-pop-up .mg-step-blocked:before{content:"";z-index:1;display:block;position:absolute;left:0;right:0;bottom:0;top:0;background:rgba(255,255,255,.9);}
#monogramming-pop-up .mg-step-blocked:after{content:"N/A";z-index:2;display:inline-block;position:absolute;left:0;right:0;top:50%;height:50px;line-height:50px;margin-top:-25px;text-align:center;font-size:30px;color:#0d50d8}

#monogramming-pop-up #mg-preview{display:block; width:100%;padding-top:100%;border: 1px solid #ddd;margin-bottom:20px;position:relative;overflow:hidden;font-size:80px;background:url({{ "mg-background-wood.jpg" | asset_url }}) no-repeat scroll center center;background-size:cover;}
#monogramming-pop-up #mg-preview span.mg-container{position:absolute;margin-top:-100px;top:50%;left:0;right:0;text-align:center;height:200px;line-height:200px;color:#622B00;opacity:.7;}
#monogramming-pop-up #mg-preview span.mg-container.mg-color-light{color:#EBB380;}
#monogramming-pop-up #mg-preview span.mg-container.mg-2-lines{line-height:1.2em;}

#monogramming-pop-up #mg-preview.mg-style-1-monogram-oval {font-size:200px;text-transform:uppercase;}
#monogramming-pop-up #mg-preview.mg-style-1-monogram-oval span.mg-letter-1{font-family:"MonogramOval1";}
#monogramming-pop-up #mg-preview.mg-style-1-monogram-oval span.mg-letter-2{font-family:"MonogramOval2";}
